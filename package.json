{
  "name": "mcp-server-sandbox",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsx -r dotenv/config src/server.ts",
    "build": "tsc",
    "dev": "tsx watch -r dotenv/config src/server.ts",
    "dev:stdio": "DOTENV_CONFIG_PATH=.env.stdio tsx watch -r dotenv/config src/server.ts",
    "dev:http": "DOTENV_CONFIG_PATH=.env.http tsx watch -r dotenv/config src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "inspector:http": "./node_modules/.bin/mcp-inspector --config mcp-inspector-config.json --server http-server",
    "inspector:stdio": "./node_modules/.bin/mcp-inspector --no-open --config mcp-inspector-config.json --server stdio-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "google-auth-library": "^10.1.0",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pino": "^7.0.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
